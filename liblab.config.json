{
	"sdkName": "celitech",
	"specFilePath": "swagger.yaml",
	"languages": [
    "python"
  ],
  "createDocs": false,
  "customizations": {
    "generateEnv": true,
    "devContainer": true,
    "license": {
      "type": "MIT"
    },
    "retry": {
      "enabled": true,
      "retryDelay": 150,
      "maxAttempts": 3
    },
    "injectedModels": [],
    "inferServiceNames": false,
    "responseHeaders": false,
    "environmentVariables" : [
      {
        "name": "CLIENT_ID",
        "description": "Client ID parameter"
      },
      {
        "name": "CLIENT_SECRET",
        "description": "Client Secret parameter"
      }
    ],
  "environments" : [
    {
      "name": "TOKEN_SERVER",
      "url": "https://auth.celitech.net/oauth2/token"
    },
    {
      "name": "PRODUCTION",
      "url": "https://api.celitech.net/v1"
    }
  ]
},
  "languageOptions": {
    "Python": {
      "pypiPackageName": "",
      "githubRepoName":"Test-SDK-Python",
      "sdkVersion": "1.0.0",
      "ignoreFiles": []
    }
  },
  "publishing": {
    "githubOrg": "saerelmasri"
  }
  
}
